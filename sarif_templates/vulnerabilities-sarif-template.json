{
    "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
    "version": "2.1.0",
    "runs": [
        {
            "tool": {
                "driver": {
                    "name": "Jforg-{report_type}-{category}",
                    "semanticVersion": "v8.0.0",
                    "informationUri": "https://www.jfrog.com/",
                    "rules": [
                        {
                            "id": "{{cve}}-{{issue_id}}",
                            "name": "{{cve}}-{{issue_id}}",
                            "helpUri": "",
                            "help": {
                                "text": "{{summary}}",
                                "markdown": "{{summary}}"
                            },
                            "fullDescription": {
                                "text": "{{summary}}"
                            },
                            "x-metadata": {
                                "cwe": "{{cwe}}",
                                "cvss": "{{cvss}}",
                                "project": "{{project_keys}}"
                            },
                            "properties": {
                                "security-severity": "{{severity}}",
                                "name": "{{cve}}-{{issue_id}}",
                                "id": "{{cve}}-{{issue_id}}",
                                "description": "{{summary}}",
                                "tags": [ "{{category}}" ]
                            }
                        }
                    ]
                }
            },
            "results": [
                {
                    "message": {
                        "text": "{{summary}}"
                    },
                    "ruleId": "{{cve}}-{{issue_id}}",
                    "locations": [
                        {
                            "physicalLocation": {
                                "artifactLocation": {
                                    "uri": "{{file_path}}"
                                },
                                "region": {
                                    "startLine": "{{component_physical_path}}",
                                    "startColumn": "{{component_physical_path}}",
                                    "endLine": "{{component_physical_path}}",
                                    "endColumn": "{{component_physical_path}}",
                                    "snippet": {
                                        "text": "{{component_physical_path}}"
                                    }
                                }
                            }
                        }
                    ],
                    "x-metadata": {
                        "impacted_artifact": "{{impacted_artifact}}",
                        "impact_path": "{{impact_path}}",
                        "fixed_versions": "{{fixed_versions}}",
                        "package_type": "{{package_type}}"
                    },
                    "partialFingerprints": {
                        "commitSha": "",
                        "email": "",
                        "author": "",
                        "date": "",
                        "commitMessage": ""
                    },
                    "properties": {
                        "tags": [ "{{category}}" ]
                    }
                }
            ]
        }
    ],
    "invocations": [
        {
            "executionSuccessful": true,
            "notificationConfigurationOverrides": [],
            "ruleConfigurationOverrides": [],
            "toolConfigurationNotifications": [],
            "toolExecutionNotifications": [],
            "workingDirectory": {
                "index": -1,
                "uri": "{{path}}"
            }
        }
    ]
}